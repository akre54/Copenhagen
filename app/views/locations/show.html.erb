<p id="notice"><%= notice %></p>

<h2><%= @location.name %></h2>

<p>
  <b>Bikes:</b>
  <ul>
  <% @location.bikes.each do |bike| %>
    <li><%= link_to "##{bike.id} (#{bike.manufacturer} #{bike.model})", bike %></li>
  <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_location_path(@location) %> |
<%= link_to 'Back', locations_path %>
